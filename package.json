{
  "name": "sebabiblio-consola",
  "version": "1.0.0",
  "description": "Consola de Administración - SebaBiblio",
  "author": "Sebastián Bertello",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^1.12.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.sebabiblio.consola",
    "productName": "SebaBiblio-Consola",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "EJECUTE_ACA_SistemaBertello.exe"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "portable": {
      "artifactName": "EJECUTE_ACA_SistemaBertello.exe"
    },
    "asar": true,
    "publish": null
  }
}
